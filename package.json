{
  "main": "mern ecommerce",
  "version": "1.0.0",
  "description": "MERN ecommerce",
  "scripts": {
    "client": "cd client && npm run dev",
    "server": "cd server && npm run dev",
    "dev": "concurrently -n \"client,server\" -c \"bgBlue,bgYellow\" \"npm run client\" \"npm run server\"",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "keywords": [
    "node",
    "express",
    "mongoose",
    "react",
    "redux"
  ],
  "author": "Mohammad Zeeshan (https://github.com/thezeeshann)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thezeeshann/ecommerce-mern/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thezeeshann/ecommerce-mern"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
